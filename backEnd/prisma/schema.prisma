generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model autok{
  id Int @id @default(autoincrement())
  gyartasEve Int
  model String
  marka String
  tomeg Float
  megtettKm Int
  Kedencek kedvencek[]
  Teszezetesek tesztVezetesek[]

}


model users{
  id Int @id @default(autoincrement())
  name String
  passsword String
  accessLLevel Int
  Kedencek kedvencek[]
  Teszezetesek tesztVezetesek[]
  Kivansaglista kivansagLlista[]
}

model kedvencek{
id Int @id @default(autoincrement())
Autok   autok @relation(fields: [autoId], references: [id])
Users users @relation(fields: [userId], references: [id])
autoId Int
userId Int

}

model tesztVezetesek{
  id Int @id @default(autoincrement())
  Autok   autok @relation(fields: [autoId], references: [id])
  Users   users @relation(fields: [userId], references: [id])
  datum DateTime
  userId Int
  autoId Int
}

model kivansagLlista{
  id Int @id @default(autoincrement())
  Users   users @relation(fields: [userId], references: [id])
  userId Int
  kivantMarka String
  kivantSebValto String
  kivantMegtettKm Int
}

